{
  "name": "gamesfinder",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "spa",
    "server",
    "shared"
  ],
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:spa": "npm --workspace spa run dev",
    "dev:server": "npm --workspace server run dev",
    "build": "npm-run-all build:*",
    "build:spa": "npm --workspace spa run build",
    "build:server": "npm --workspace server run build",
    "build:shared": "npm --workspace shared run build"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
