{  "name":"gamesfinder",  "version":"1.0.0",  "private":true,  "workspaces":[    "spa",    "server",    "shared"  ],  "scripts":{    "dev":"npm-run-all -p dev:*",    "dev:server":"dotenv -e .env -- npm --workspace server run dev",    "dev:spa":"dotenv -e .env -- npm --workspace spa run dev",    "build":"npm-run-all build:shared build:spa build:server",    "build:shared":"npm --workspace=shared run build",    "build:spa":"npm --workspace=spa run build",    "build:server":"npm --workspace=server run build",    "preview":"../node_modules/.bin/vite preview",    "lint":"../node_modules/.bin/eslint . --ext .ts,.vue",    "lint:fix":"../node_modules/.bin/eslint . --ext .ts,.vue --fix"  },  "devDependencies":{    "dotenv-cli":"^8.0.0",    "npm-run-all":"^4.1.5"  },"dependencies":{    "dotenv":"^16.5.0"  }}